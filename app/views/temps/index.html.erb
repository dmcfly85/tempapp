<h1>Listing temps</h1>
<%=@temps.count%>

<table class="table table-condensed">
  <thead>
    <tr>
      <th>Insidetemp</th>
      <th>Outsidetemp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @temps.each do |temp| %>
      <tr>
        <td><%= temp.insidetemp %></td>
        <td><%= temp.outsidetemp %></td>
        <td><%= temp.created_at.strftime("%H:%M:%S") %></td>
        <td><%= link_to 'Edit', edit_temp_path(temp) %></td>
        <td><%= link_to 'Destroy', temp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
